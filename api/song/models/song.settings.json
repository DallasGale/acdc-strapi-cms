{
  "connection": "default",
  "collectionName": "songs",
  "info": {
    "name": "song"
  },
  "options": {
    "increments": true,
    "timestamps": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "length": {
      "type": "string",
      "default": "00:MM:SS:000",
      "minLength": 12,
      "maxLength": 12
    },
    "writingCredits": {
      "type": "json"
    },
    "single": {
      "type": "boolean",
      "default": false
    },
    "singleReleaseDate": {
      "type": "date"
    },
    "bSide": {
      "type": "component",
      "repeatable": true,
      "component": "single-release.b-side"
    }
  }
}
