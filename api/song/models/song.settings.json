{
  "connection": "default",
  "collectionName": "songs",
  "info": {
    "name": "song"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "writer": {
      "type": "component",
      "repeatable": true,
      "component": "ownership.writer"
    },
    "length": {
      "type": "string",
      "default": "HH:MM:SS:ZZZ",
      "minLength": 12,
      "maxLength": 12
    }
  }
}
