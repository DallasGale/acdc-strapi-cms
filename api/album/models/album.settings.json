    },
    "isNorthAmericanVersion": {
      "type": "boolean"
    },
    "releaseDate": {
      "type": "date"
    },
    "isJapaneseVersion": {
      "type": "boolean"
    },
    "sideC": {
      "type": "component",
      "repeatable": true,
      "component": "long-play-format.side-c"
    },
    "sideD": {
      "type": "component",
      "repeatable": true,
