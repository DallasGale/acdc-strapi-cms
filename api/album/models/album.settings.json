    "isLiveRecording": {
      "type": "boolean",
      "default": false
    },
    "isInternationalVersion": {
      "type": "boolean",
      "default": false
    },
    "isAustralianVersion": {
      "type": "boolean",
      "default": false
    },
    "isCompilation": {
      "type": "boolean",
      "default": false
    },
    "isSoundtrack": {
      "type": "boolean",
      "default": false
    },
    "sideA": {
      "type": "component",
      "repeatable": true,
      "component": "long-play-format.side-a"
    },
    "sideB": {
      "type": "component",
      "repeatable": true,
      "component": "long-play-format.side-a"
    },
    "producers": {
      "collection": "producer"
    },
    "performers": {
      "collection": "performer"
    },
    "charts": {
      "type": "component",
      "repeatable": true,
      "component": "certified.peak-chart-position"
