{
  "connection": "default",
  "collectionName": "components_single_release_album_singles",
  "info": {
    "name": "albumSingle",
    "icon": "map-signs"
  },
  "options": {},
  "attributes": {
    "details": {
      "type": "string"
    },
    "coverArt": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "industryCharts": {
      "type": "component",
      "repeatable": true,
      "component": "certified.peak-chart-position"
    },
    "released": {
      "type": "date"
    },
    "bSide": {
      "collection": "song"
    },
    "releaseYear": {
      "type": "integer",
      "min": 1970,
      "max": 2030
    },
    "monthReleased": {
      "type": "enumeration",
      "enum": [
        "January",
        "February",
        "March",
        "April",
        "May",
        "June",
        "July",
        "August",
        "September",
        "October",
        "November",
        "December"
      ]
    }
  }
}
